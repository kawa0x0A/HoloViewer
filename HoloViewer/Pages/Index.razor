@page "/Index"

<div>
    <div class="group">
        @foreach (var group in vTuberGroups)
        {
            <ul>
                <li>
                    @group.groupName
                    <ul>
                        @foreach (var profile in group.groupMembers)
                        {
                            <li>
                                <NavLink href="@profile.youtubePageUrl">
                                    @profile.markIcon @profile.fullName
                                </NavLink>
                            </li>
                        }
                    </ul>
                </li>
            </ul>
        }
    </div>
</div>

@code
